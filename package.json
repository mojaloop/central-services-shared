    "@mojaloop/central-services-metrics": "12.7.1",
    "@mojaloop/event-sdk": "14.7.0",
    "@mojaloop/sdk-standard-components": "19.17.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@types/hapi__joi": "17.1.15",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "ajv-keywords": "^5.1.0",
    "audit-ci": "7.1.0",
    "base64url": "3.0.1",
    "chance": "1.1.13",
