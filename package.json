    "env-var": "7.5.0",
    "event-stream": "4.0.1",
    "fast-safe-stringify": "2.1.1",
    "immutable": "5.1.3",
    "ioredis": "5.7.0",
    "joi": "18.0.1",
    "lodash": "4.17.21",
    "mustache": "4.2.0",
    "openapi-backend": "5.15.0",
    "raw-body": "3.0.1",
    "rc": "1.2.8",
    "redlock": "5.0.0-beta.2",
    "shins": "2.6.0",
    "ulidx": "2.4.1",
    "uuid4": "2.0.3",
    "widdershins": "4.0.1",
    "yaml": "2.8.1"
  },
  "devDependencies": {
    "@mojaloop/central-services-error-handling": "13.1.2",
